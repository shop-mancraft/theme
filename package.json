  "private": true,
  "homepage": "https://mancraft.pl/",
  "repository": {
    "type": "git",
    "url": "https://github.com/webo-agency/mancraft.git"
  },
  "bugs": {
    "url": "https://github.com/webo-agency/mancraft/issues"
